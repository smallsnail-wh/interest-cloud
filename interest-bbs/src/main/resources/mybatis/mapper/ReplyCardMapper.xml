<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.interest.bbs.dao.ReplyCardDao">
	<resultMap id="replyCardMap"
		type="com.interest.bbs.model.entity.ReplyCardEntity">
		<id column="id" property="id" />
		<result column="userid" property="userid" />
		<result column="content" property="content" />
		<result column="postcardid" property="postcardid" />
		<result column="createtime" property="createtime" />
	</resultMap>

	<resultMap id="replyCardModelMap"
			   type="com.interest.bbs.model.response.ReplyCardResponse">
		<id column="id" property="id" />
		<result column="username" property="username" />
		<result column="content" property="content" />
		<result column="postcardid" property="postcardid" />
		<result column="createtime" property="createtime" />
		<result column="headimg" property="headimg" />
		<result column="githuburl" property="githuburl" />
	</resultMap>

</mapper>